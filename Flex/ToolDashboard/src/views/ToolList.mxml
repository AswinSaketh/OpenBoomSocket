<?xml version="1.0" encoding="utf-8"?>
<mx:Canvas styleName="toolAccList" xmlns:mx="http://www.adobe.com/2006/mxml" creationComplete="init()" width="100%" height="100%">
	<mx:Script>
		<![CDATA[
			import mx.collections.ArrayCollection;
			import mx.core.Application;
			
			[Bindable]
			private var appObj:Object;
			[Bindable]
			public var dataList:ArrayCollection;
			
			private function init():void{
				appObj = Application.application;
			}
		]]>
	</mx:Script>
	<mx:TileList id="listHolder" styleName="listHolder" columnCount="2" itemRenderer="renderers.ToolSelectionRenderer"
		dataProvider="{dataList}" width="100%" height="100%">
	</mx:TileList>
</mx:Canvas>
